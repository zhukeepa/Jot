{
  "manifest_version": 2,

  "name": "Jot",
  "description": "Jot quick notes unobtrusively!",
  "version": "1.0",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "options_page": "options.html",
  "permissions": ["storage", "http://localhost:3000/*", "http://jot-mailer.herokuapp.com/*"]
}